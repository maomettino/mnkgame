    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Attach MNKGame",
            "request": "attach",
            "mainClass": "mnkgame.MNKGame",
             "processId": "${command:PickJavaProcess}"
        },
        {
            "type": "java",
            "name": "Launch MNKGame",
            "request": "launch",
            "mainClass": "mnkgame.MNKGame",
            "args": "5 5 4 mnkgame.SaddamHussein"
        },
        {
            "type": "java",
            "name": "Launch MNKPlayerTester",
            "request": "launch",
            "mainClass": "mnkgame.MNKPlayerTester"
        }
    ]
}